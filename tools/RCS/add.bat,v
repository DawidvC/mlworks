1.5
log
@[Bug #1668]
Allow adding to non trunk branches
@
text
@d6 4
d25 4
a28 22
set coopts=
set dateopts=-ci-date
:more_parameters
  if "%1" == "" goto no_more_parameters
  if not "%1" == "-b" goto ok3
    shift
    set coopts= %coopts% -branch %1
    shift
    goto more_parameters
:ok3
:no_more_parameters
if "%1"=="" goto bad_parms
:loop
  pushd .
  call findcomp %1
  if "%compound%"=="crap" goto bad_file
  call hope add %coopts% -filedate -compound %compound% -unit %unit%
  goto continue
:bad_file
echo file %1 cannot be added
goto continue
:continue
d30 3
a32 6
popd
if not "%1"=="" goto loop
goto done
:bad_parms
echo Missing parameters ^<files to add^>
:done
@


