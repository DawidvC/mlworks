1.1
log
@new unit
NT version of unix script
@
text
@d5 5
a9 1
rem $Log$
d15 4
a18 21
set coopts=
:more_parameters
  if "%1" == "" goto no_more_parameters
  if not "%1" == "-b" goto ok1
    shift
    set coopts= %coopts% -branch %1
    shift
    goto no_more_parameters
:ok1
:no_more_parameters
if "%1"=="" goto bad_parms
:loop
  pushd .
  call findcomp %1
  if "%compound%"=="crap" goto bad_file
  call hope abandon %coopts% -compound %compound% -unit %unit%
  goto continue
:bad_file
echo file %1 cannot be abandoned
goto continue
:continue
d20 3
a22 6
popd
if not "%1"=="" goto loop
goto done
:bad_parms
echo Missing parameters ^<files to abandon^>
:done
@


