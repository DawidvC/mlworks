1.5
log
@Adding -n option
@
text
@d6 3
d27 4
a30 43
set coopts=
set dateopts=-ci-date
:more_parameters
  if "%1" == "" goto no_more_parameters
  if not "%1" == "-s" goto ok1
    shift
    set coopts=%coopts% -claim soft
    goto more_parameters
:ok1
  if not "%1" == "-h" goto ok2
    shift
    set coopts= %coopts% -claim hard
    goto more_parameters
:ok2
  if not "%1" == "-b" goto ok3
    shift
    set coopts= %coopts% -branch %1
    shift
    goto more_parameters
:ok3
  if not "%1" == "-d" goto ok4
    shift
    set coopts= %coopts% -date %1
    shift
    goto more_parameters
:ok4
  if not "%1" == "-n" goto ok5
    shift
    set dateopts=
    goto more_parameters
:ok5
:no_more_parameters
if "%1"=="" goto bad_parms
:loop
  pushd .
  call findcomp %1
  if "%compound%"=="crap" goto bad_file
  call hope co %dateopts% %coopts% -missing-dir abort -compound %compound% -unit %unit%
  goto continue
:bad_file
echo file %1 cannot be checked out
goto continue
:continue
d32 3
a34 6
popd
if not "%1"=="" goto loop
goto done
:bad_parms
echo Missing parameters ^<files to checkout^>
:done
@


