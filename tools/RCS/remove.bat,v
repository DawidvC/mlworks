1.2
log
@[Bug #1669]
Add -b option to specify branch
@
text
@d6 4
d17 4
a20 21
set coopts=
:more_parameters
  if "%1" == "" goto no_more_parameters
  if not "%1" == "-b" goto ok3
    shift
    set coopts= %coopts% -branch %1
    shift
    goto more_parameters
:ok3
:no_more_parameters
if "%1"=="" goto bad_parms
:loop
  pushd .
  call findcomp %1
  if "%compound%"=="crap" goto bad_file
  call hope remove %coopts% -compound %compound% -unit %unit%
  goto continue
:bad_file
echo file %1 cannot be removed
goto continue
:continue
d22 3
a24 6
popd
if not "%1"=="" goto loop
goto done
:bad_parms
echo Missing parameters ^<files to remove^>
:done
@


