1.2.1.1
date	92.01.02.11.52.39;	author jont;	state Exp;
branches;
next	;


desc
@Like coall, but checks out the old ten15 stuff as well
@


1.2
log
@Changed file to use RCS version 5.6.
@
text
@#!/bin/csh
# $Log: coallall,v $
# Revision 1.1  1991/12/19  17:00:28  jont
# Initial revision
#
# Copyright (c) 1991 Harlequin Ltd.
if ($#argv != 0) then
  echo Usage: coallall
  exit(1)
endif
coall
set dirs = {codegen,encoder}
set nonomatch
foreach i ($dirs)
  echo Checking directory $i
  pushd $i > /dev/null
  ls RCS/*,v
  ~sml/tools/cout RCS/*,v
  popd > /dev/null
end
@


1.2.1.1
