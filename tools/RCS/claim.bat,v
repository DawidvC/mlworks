1.4
log
@Add ability to give a bug number
@
text
@d6 3
d24 4
a27 37
set coopts=
:more_parameters
  if "%1" == "" goto no_more_parameters
  if not "%1" == "-s" goto ok1
    shift
    set coopts=%coopts% -soft
    goto more_parameters
:ok1
  if not "%1" == "-h" goto ok2
    shift
    set coopts=%coopts% -hard
    goto more_parameters
:ok2
  if not "%1" == "-u" goto ok3
    shift
    set coopts=%coopts% -update-reason
    goto more_parameters
:ok3
  if not "%1" == "-i" goto ok4
    shift
    set coopts=%coopts% -bug-number %1
    shift
    goto more_parameters
:ok4
:no_more_parameters
if "%1"=="" goto bad_parms
:loop
  pushd .
  call findcomp %1
  echo off
  if "%compound%"=="crap" goto bad_file
  call hope claim %coopts% -compound %compound% -unit %unit%
  goto continue
:bad_file
echo file %1 cannot be claimed
goto continue
:continue
d29 3
a31 6
popd
if not "%1"=="" goto loop
goto done
:bad_parms
echo Missing parameters ^<files to claim^>
:done
@


