# Install the foreign examples.  Note that the foreign rule doesn't do
# the usual :-
#
#	-$(MKDIR) $(DISTDIR)/examples	
#	$(chmod_exec) $(DISTDIR)/examples
#	$(RM) $(DISTDIR)/examples/foreign
#	-$(MKDIR) $(DISTDIR)/examples/foreign
#	$(chmod_exec) $(DISTDIR)/examples/foreign
#
# because this has already been done as part of installing mlw_ci.h
# by the rts installer <URI:hope://MLW/src/rts/GNUmakefile#distribution>

foreign: force
	( cd foreign; ./dist_foreign_files.sh $(DISTDIR)/examples/foreign $(OS) $(ARCH) )


@


