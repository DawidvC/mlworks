require "myarray";

signature NUMREGEXP =
  sig

    structure Array : ARRAY 

    type NumRegExp

    val number: RegExp -> NumRegExp

    val nullable : NumRegExp -> bool
    val firstpos : NumRegExp -> int list
    val lastpos : NumRegExp -> int list
    val followpos : NumRegExp -> int list Array.array
    val root : NumRegExp -> NumRegExp

end
@


