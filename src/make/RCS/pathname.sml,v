desc
@Unix Pathname routines
@


1.2
log
@Added copyright message
@
text
@(* pathname.sml the signature *)
(*
$Log:	pathname.sml,v $
Revision 1.1  91/06/07  11:05:02  colin
Initial revision

Copyright (c) 1991 Harlequin Ltd.
*)
signature PATHNAME =
  sig

    type Directory and PathName

    val pathname_type : PathName -> string
    val pathname_name : PathName -> string
    val pathname_directory : PathName -> Directory
    val modify_type : (PathName * string) -> PathName
    val modify_name : (PathName * string) -> PathName
    val modify_directory : (PathName * Directory) -> PathName

    val parseDirectory : string -> Directory
    val directory_namestring : Directory -> string

    exception ParsePathName
    val parsePathName : string -> PathName
    val pathname_namestring : PathName -> string

    val isRelative : Directory -> bool
    val resolveRelative : Directory * Directory -> Directory

  end
      
@


