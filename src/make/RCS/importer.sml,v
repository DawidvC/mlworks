@(* importer.sml the signature *)
(*
$Log: importer.sml,v $
Revision 1.7  1991/11/21  16:31:28  jont
Added copyright message

Revision 1.6  91/06/24  11:32:34  colin
Changed result type of exporML from unit to bool (as in original NJML defn)

Revision 1.5  91/06/13  21:26:00  colin
Added checkout_old : bool ref. Controls whether on seeing a file with a time
stamp earlier in database we should just recompile or try and checkout latest

Revision 1.4  91/06/11  17:08:05  colin
added reference co_command to allow switching of rcs checkout command

Revision 1.3  91/06/10  17:32:09  colin
added rcs check out function

Revision 1.2  91/06/07  19:15:30  colin
Removed old aliasing facility as no longer needed due to RCS

Revision 1.1  91/06/07  11:03:37  colin
Initial revision

Copyright (c) 1991 Harlequin Ltd.
*)

signature IMPORTER =
  sig
    val print : string -> unit

    val cd  : string -> string
    val pwd : unit   -> string
    val use : string -> unit
      
    val make    : string -> unit
    val again   : unit   -> unit
    val require : string -> unit
    val makeok  : string -> unit

    val exportML : string -> bool
  end

@


