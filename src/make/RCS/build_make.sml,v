1.3
log
@Adjusted the initial timer functions to make smlk images with this loaded
in compatible with our own compiler
@
text
@d4 4
d29 1
a29 4
fun require x = 
  if x = "string"
    then use "../utils/string.sml"
  else use (x ^ ".sml");
d31 1
a31 1
use "redefine_timer_functions.sml" ;
a32 1
use "unix.sml";
d36 1
a36 1
use "_unix.sml";
d40 2
a41 7
use "__unix.sml";

structure PathName = PathName (structure Unix = Unix_)
structure Importer = Importer (structure Unix = Unix_ and PathName = PathName)

open Importer

d43 1
@


