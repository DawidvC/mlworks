require "datatypes";
require "nameset";

signature NEWSIGMA =
  sig
    structure Datatypes : DATATYPES
    structure Nameset : NAMESET

    sharing Datatypes = Nameset.Datatypes

    val new_names_of : Datatypes.Str * Nameset.Nameset -> Nameset.Nameset
    val names_of : Datatypes.Str * Nameset.Nameset -> Nameset.Nameset
    val names_of_env : Datatypes.Env * Nameset.Nameset -> Nameset.Nameset
  end
@


