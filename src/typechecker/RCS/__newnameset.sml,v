desc
@New implementation of nameset using hashed sets as underlying implementation
@


1.2
log
@Removed redundant Array argument and require.
@
text
@(* __newnameset.sml the structure *)
(*
$Log: __newnameset.sml,v $
Revision 1.1  1992/04/21  17:20:19  jont
Initial revision

Copyright (c) 1992 Harlequin Ltd.
*)

require "../utils/__debug";
require "../utils/__set";
require "../utils/_hashset";
require "../utils/__crash";
require "../utils/__lists";
require "../utils/__print";
require "__ty_debug";
require "__datatypes";
require "__types";
require "__strnames";
require "__namehash";
require "_newnameset";

structure NewNameset_ = NewNameset(
  structure Set       = Set_
  structure TynameSet = HashSet(
    structure Crash = Crash_
    structure Lists = Lists_
    type element = Datatypes_.Tyname
    val eq = Types_.tyname_eq
    val hash = NameHash_.tyname_hash
    val size = 1024
  )
  structure StrnameSet = HashSet(
    structure Crash = Crash_
    structure Lists = Lists_
    type element = Datatypes_.Strname
    val eq = Strnames_.strname_eq
    val hash = NameHash_.strname_hash
    val size = 512
  )
  structure Debug     = Nameset_Debug_
  structure Print     = Print_
  structure Ty_Debug = Ty_Debug_
  structure Datatypes = Datatypes_
  structure Types     = Types_
  structure Strnames  = Strnames_
);
@


