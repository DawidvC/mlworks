 *
 * Revision 1.1  1994/12/09  13:33:56  jont
 * new file
 *
 *
 *)

require "_unixgetenv";
require "__unixos";

structure Getenv_ =
  UnixGetenv (structure UnixOS = UnixOS_)
@


