1.3
log
@[Bug #20025]
Correct spelling of delimiter
@
text
@d17 1
a17 10
delimiter=@@
while
  case $* in
    *$delimiter*) true;;
    *) false;;
  esac
do
  delimiter=$delimiter@@
done
exec $BINDIR/mlrun -stack 5 $RUNTIMEOPTS $MONO -load $LOAD_FILE -pass $delimiter $* $delimiter
@


