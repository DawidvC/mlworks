1.2
log
@Change the checkout file mode
@
text
@d17 1
a17 1
delimeter=@@
d20 1
a20 1
    *$delimeter*) true;;
d24 1
a24 1
  delimeter=$delimeter@@
d26 1
a26 1
exec $BINDIR/mlrun -stack 5 $RUNTIMEOPTS $MONO -load $LOAD_FILE -pass $delimeter $* $delimeter
@


