@@@echo off
set INSTALL=DRIVEINSTALLDIR
@@rem collect the arguments
@@rem if DOS wasn't so poor we wouldn't have to do this
set MYARGS=
:startmyargs
if "%1"=="" goto endmyargs
set MYARGS=%MYARGS% %1
shift
goto startmyargs
:endmyargs
@


