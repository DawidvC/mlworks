@d12 5
a46 55


(*

structure General : GENERAL =
  struct

    type exn = exn
    type unit = unit

    exception Bind  = Bind
    exception Chr = Chr
    exception Match = Match
    exception Subscript = Subscript
    exception Size = Size
    exception Overflow = Overflow
    exception Domain
    exception Div = Div
    exception Fail of string


    exception Option
    open MLWorks.Option
    (* Provides the datatypes option and union *)

    fun getOpt (NONE, dflt) = dflt
      | getOpt (SOME x, _)  = x

    fun isSome NONE     = false
      | isSome (SOME _) = true

    fun valOf NONE     = raise Option
      | valOf (SOME x) = x

    datatype order = LESS | EQUAL | GREATER

    val exnName = MLWorks.Internal.Value.exn_name_string o MLWorks.Internal.Value.exn_name

    val exnMessage = exnName

    val (op<>) : ''a * ''a -> bool = (op<>)

    val !  : 'a ref -> 'a = !
      
    val (op :=) : ('a ref * 'a) -> unit = (op :=)
      
    val (op o)  : ('b -> 'c) * ('a -> 'b) -> ('a -> 'c) = (op o)
      
    val before : ('a * 'b) -> 'a = fn (x,_) => x
      
    val ignore : 'a -> unit = fn (_) => ()

  end

*)
@


