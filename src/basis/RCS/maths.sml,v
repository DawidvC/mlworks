signature MATH =
  sig
    type real

    exception Sqrt
    exception Trig
    exception Ln

    val pi : real
    val e : real

    val sqrt : real -> real
    val sin : real -> real
    val cos : real -> real
    val tan : real -> real
    val atan : real -> real
    val asin : real -> real
    val acos : real -> real
    val atan2 : real * real -> real
    val exp : real -> real
    val pow : real * real -> real
    val ln : real -> real
    val log10 : real -> real

  end (* signature MATH *)
@
