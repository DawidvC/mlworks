signature VECTOR = 
  sig
      eqtype 'a vector
      val maxlen   : int
      val vector   : 'a list -> 'a vector
      val tabulate : int * (int -> 'a) -> 'a vector
      val length   : 'a vector -> int
      val sub      : 'a vector * int -> 'a 
      val extract  : ('a vector * int * int) -> 'a vector
      val concat   : 'a vector list -> 'a vector
  end
@
