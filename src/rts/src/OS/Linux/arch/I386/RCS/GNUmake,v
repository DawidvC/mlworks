1.9
log
@Add OS_TYPE = Unix so that files common to all UNIX variants can be
picked up from src/rts/src/OS/Unix
@
text
@d1 8
d20 1
a20 2
DEFINES		:= $(DEFINES) -DMACH_FIXUP
#		-DMACH_FIXUP -DMACH_INT_MUL -DMACH_POLY_EQ -DMACH_STRINGS
d22 1
d24 1
a24 1
# Linux is a type of Unix ...
d26 1
a26 1
OS_TYPE := Unix
d28 2
a29 2
# Note that this must come before the following since INCLUDES
# is defined in terms of OS_TYPE
d31 1
d33 2
a34 5
# add target-specific includes

INCLUDES	:= $(INCLUDES) -I-

# add target-specific libraries
d36 1
a36 1
LIBRARIES	:= $(LIBRARIES) -lc
d38 1
a38 1
STRIP		:= /usr/bin/strip
@


