1.15
log
@Add OS_TYPE = Unix so that files common to all UNIX variants can be
picked up from src/rts/src/OS/Unix
@
text
@d3 1
a3 1
# RCS Id: $Id: src:OS:Solaris:arch:SPARC:GNUmake,v 1.14 1996/02/08 15:32:42 jont Exp $
d6 4
d21 3
a23 6
		-DMACH_FIXUP -DMACH_INT_MUL -DMACH_INT_DIV -DMACH_POLY_EQ \
		-DMACH_STRINGS -DIMPLICIT_PROFILE_CODE 

# add target specific compiler options

CCOPTS		:= $(CCOPTS) -Wconversion
d25 1
a25 6
# Solaris is a Unix variant ...

OS_TYPE := Unix

# Note that this must come before the following since INCLUDES
# is defined in terms of OS_TYPE
d27 1
d29 2
a30 3
# add target-specific includes

INCLUDES	:= $(INCLUDES) -I-
d34 2
a35 4
LIBRARIESSTATIC	:= $(LIBRARIES) \
		-lXm -lXt -lX11 -lsocket -lnsl -lgen -lform -lintl -lelf

LIBRARIES	:= $(LIBRARIESSTATIC) -ldl
d39 1
a39 1
STRIP		:= /usr/ccs/bin/strip
@


