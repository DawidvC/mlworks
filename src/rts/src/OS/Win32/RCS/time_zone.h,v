#ifndef time_zone_h
#define time_zone_h

#include <windows.h>

#define double_time(t) (((double)t.dwHighDateTime * two_to_32) + ((double)t.dwLowDateTime))

extern void ml_stat_sub(const char *filename, FILETIME *);

#endif /* !time_zone_h */
