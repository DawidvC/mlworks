@Change CLK_TCK to CLOCKS_PER_SEC
@
text
@/*  ==== ANSI COMPATABILITY HEADER ====
 *
 *  Copyright (C) 1992 Harlequin Ltd
 *
 *  Description
 *  -----------
 *  This header defines some things which are part of the ANSI standard but
 *  missing from the compiler/environment.
 *
 *  $Id: src:ansi.h,v 1.2 1994/06/09 14:32:38 nickh Exp $
 */

#ifndef ansi_h
#define ansi_h

#include <stdarg.h>
#include <stdio.h>
#include <sys/types.h>
#include <time.h>

#ifdef __GNUC__
#define EXIT_SUCCESS	0
#define EXIT_FAILURE	1
#define FILENAME_MAX	1024	/* from the manual */
#define FOPEN_MAX	0	/* guaranteed number */
#define CLOCKS_PER_SEC	1000000
#define SEEK_SET	0
#define SEEK_CUR	1
#define SEEK_END	2
#endif /* __GNUC__ */

#endif
@


