                MENUITEM "&Miscellaneous",  IDM_MISC,     GRAYED
        }

        POPUP "&Options"
        {
                MENUITEM "&Mode..",         IDM_MODE,     GRAYED
                MENUITEM "&Value Printer",  IDM_VALUE,    GRAYED
        }

        POPUP        "&Help"
        {
                MENUITEM "&Contents",               IDM_HELPCONTENTS
                MENUITEM "&Search for Help On...",  IDM_HELPSEARCH
                MENUITEM "&How to Use Help",        IDM_HELPHELP
                MENUITEM SEPARATOR
                MENUITEM "&About MLWorks...",       IDM_ABOUT
        }
END

