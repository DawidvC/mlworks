desc
@Object file structure
@



1.1
log
@Initial revision
@
text
@/*
 * ml.out.h
 * object file structure
 * $Log$
 */

#ifndef ml_out_h /* Avoid multiple inclusion */
#define ml_out_h

#ifndef types_h /* For efficiency */
#include "types.h"
#endif

typedef struct ml_header {
  u_int32 magic;
  u_int32 version;
  u_int32 code_size;
  u_int32 env_size;
  u_int32 debug_size;
} ml_header;

#define OFFSET_CODE (sizeof(struct ml_header))
#define OFFSET_ENV(x) (OFFSET_CODE + sizeof(u_int32)*((x)->code_size))
#define OFFSET_DEBUG(x)  (OFFSET_ENV(x) + sizeof(u_int32)*((x)->env_size))

#endif
/* End ml.out.h */
@


