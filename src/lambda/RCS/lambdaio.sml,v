@(*  ==== LAMBDA CODE I/O ====
 *          SIGNATURE
 *
 *  Copyright (C) 1993 Harlequin Ltd.
 *
 *  Description
 *  -----------
 *  This module provides code for the input and output of lambda expressions
 *  into files.  It is primarily intended for use in the COMPARE project.
 *
 *  Revision Log
 *  ------------
 *  $Log: lambdaio.sml,v $
 *  Revision 1.1  1993/08/23  15:19:59  richard
 *  Initial revision
 *
 *)

require "lambdatypes";

signature LAMBDAIO =
  sig
    structure LambdaTypes : LAMBDATYPES

    exception Output of string
    val output : outstream * LambdaTypes.LambdaExp -> unit
  end;
@


