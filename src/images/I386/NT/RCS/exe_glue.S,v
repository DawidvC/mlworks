@new unit
Initial code for building executable
@
text
@/*
 * Copyright (c) 1998, Harlequin Group plc
 * All rights reserved
 *
 * $Log$
 */
	.text
	.align 4
	.globl	_main
	.globl	_trampoline
	.globl	SETUP
	.globl	DECLARE
_main:
	pushl	%ebp
	movl	%esp,%ebp
	lea	DECLARE,%edx	/* Get all the global declarations done first */
	lea	SETUP+5,%eax
	push	%edx
	push	%eax
	call	_trampoline
	add	$8,%esp
	mov	%ebx,%eax
	leave
	ret
	.align	8
@


