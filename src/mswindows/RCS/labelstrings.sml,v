signature LABELSTRINGS =
  sig
    type word
    type AcceleratorFlag

    val VK_DELETE : int

    val VK_PRIOR : int
    val VK_NEXT : int
    val VK_END : int
    val VK_HOME : int

    val VK_LEFT : int
    val VK_UP : int
    val VK_RIGHT : int
    val VK_DOWN : int

    val accelerators : (AcceleratorFlag list * int * int) list
    val get_label : string -> string 
    val get_action : string -> word
    val get_title : string -> string
  end
@


