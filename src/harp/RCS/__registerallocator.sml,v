local
a66 49
  structure GCHashTable_ =
    HashTable
    (struct
       type Key = MirTypes_.GC.T
       type Value = int
       val size = 1000
       val hash = MirTypes_.GC.hash
       val eq = op=
     end)

  structure NonGCHashTable_ =
    HashTable
    (struct
       type Key = MirTypes_.NonGC.T
       type Value = int
       val size = 10
       val hash = MirTypes_.NonGC.hash
       val eq = op=
     end)

  structure FPHashTable_ =
    HashTable
    (struct
       type Key = MirTypes_.FP.T
       type Value = int
       val size = 10
       val hash = MirTypes_.FP.hash
       val eq = op=
     end)

