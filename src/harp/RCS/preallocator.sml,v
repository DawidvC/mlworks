1.1
log
@Initial revision
@
text
@(*  ==== REGISTER PREALLOCATOR ====
 *            SIGNATURE
 *
 *  Copyright (C) 1992 Harlequin Ltd.
 *
 *  Description
 *  -----------
 *  The preallocator makes a fast examination of large blocks in an
 *  annotated MIR procedure and produces a list of pairs of registers which
 *  do not clash.  This list is passed on to the register allocator as
 *  hinting information.
 *
 *  Diagnostics
 *  -----------
 *   1  block tags processed
 *   2  mergers between registers
 *   3  sets of confined registers and register lifespans
 *
 *  Revision Log
 *  ------------
 *  $Log$
 *)


require "../utils/diagnostic";
require "mirprocedure";
require "mirtypes";


signature PREALLOCATOR =

  sig
    
    structure MirTypes		: MIRTYPES
    structure MirProcedure	: MIRPROCEDURE
    sharing MirTypes = MirProcedure.MirTypes
    structure Diagnostic	: DIAGNOSTIC

    val analyse : MirProcedure.procedure ->
      {gc     : (MirTypes.GC.T * MirTypes.GC.T) list,
       non_gc : (MirTypes.NonGC.T * MirTypes.NonGC.T) list,
       fp     : (MirTypes.FP.T * MirTypes.FP.T) list}

  end
@


1.1.1.1
