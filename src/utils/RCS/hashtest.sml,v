desc
@Test file for SML hashtable extension
@


1.2
log
@Added copyright message
@
text
@(* hashtest.sml *)
(*
$Log:	hashtest.sml,v $
Revision 1.1  91/06/07  15:58:26  colin
Initial revision

Copyright (c) 1991 Harlequin Ltd.
*)
structure Spec : HASHSPEC = 
  struct 
    type Key = int
    type Value = string
    val size = 63
    fun hash n = n
    val eq = (op =) 
  end;

structure HT = HashTable (Spec);

exception Test

val s = "hello"

fun test 0 = ()
  | test n =
    (HT.update (n,s);
     if HT.lookup (n) = s then test (n-1) else raise Test)


fun test2 0 = ()
  | test2 n =
    (HT.lookup 100;
     test2 (n-1));
    

@


