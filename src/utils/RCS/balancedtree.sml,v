  sig
    type ('key, 'item) btree
    datatype ordering = LT | EQ | GT
    datatype 'a result = YES of 'a | NO
      
    val empty  : ('key * 'key -> ordering) -> ('key, 'item) btree
    val insert : ('key, 'item) btree * 'key * 'item -> ('key, 'item) btree
    val delete : ('key, 'item) btree * 'key -> ('key, 'item) btree result
    val lookup : ('key, 'item) btree * 'key -> 'item result

    val from_alist :
      ('key * 'key -> ordering) -> ('key * 'item) list -> ('key, 'item) btree
    val to_alist   : ('key, 'item) btree -> ('key * 'item) list

    (* Delete entries satisfying a predicate. *)
    val deletep :
      ('key, 'item) btree * ('key * 'item -> bool) -> ('key, 'item) btree

  end*)
@


