(* A few basic (but non-pervasive) functions connected with integers *)

signature INTEGER = 
  sig
    val makestring : int -> string
    val max    : int * int -> int
    val min    : int * int -> int
    val print  : int -> unit
    val parseinteger : string -> int

    exception Invalid
    val getint : string -> int
  end
@


