signature FOREIGN_HEADER =
   sig

     structure FData : FOREIGN_DATA

     type 'lang_type data_port

     sharing type data_port = FData.data_port

     datatype 'lang_type sig_entry =
	 Value of (string * 'lang_type) |
	 Type  of (string * 'lang_type) |
	 Code  of (string * ('lang_type) list * ('lang_type) list) |
	 Const of (string * ('lang_type) data_port)

     type 'lang_type foreign_signature

     datatype mode = STATIC | DYNAMIC

   end; (* signature FOREIGN_HEADER *)
@
