require "../basis/__general";

signature FOREIGN_CODEINFO =
   sig

     type ('a)option = 'a General.option

     type ('entry) foreign_codeinfo


     (* foreign signature operations *)

     val new_codeinfo      : unit -> 'entry foreign_codeinfo

     val lookup_entry : 'entry foreign_codeinfo * string -> ('entry)option
     val def_entry    : 'entry foreign_codeinfo * (string * 'entry) -> unit
     val hide_entry   : 'entry foreign_codeinfo * string -> unit

     val show_entries : 'entry foreign_codeinfo -> (string * 'entry) list

   end; (* signature FOREIGN_CODEINFO *)
@


