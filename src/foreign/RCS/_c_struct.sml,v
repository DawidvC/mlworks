require "fi_types";
require "fi_struct";
require "c_struct";

functor CStruct(

     structure FITypes : FOREIGN_TYPES
  structure FIStruct : FOREIGN_STRUCTURE

) : C_STRUCTURE =
   struct

     structure FITypes = FITypes
     open FITypes

     open FIStruct

     type foreign_structure = foreign_structure
     type c_structure = foreign_structure

     val to_struct : c_structure -> foreign_structure = (fn x => x)

   end;
@
