require "__c_data";
require "__c_sig";
require "__c_hostel";
require "__c_struct";
require "__c_function";

require "_c_interface";

structure CInterface_ = CInterface(

    structure CHostel    =  CHostel_
    structure CStruct    =  CStruct_
    structure CSignature    =  CSignature_
    structure CData      =  CData_
    structure CFunction  =  CFunction_

);
@
