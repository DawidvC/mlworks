text
@d4 3
d8 2
d11 1
a11 1
XLIBS           = -L/usr/local/X11R5/lib -L/usr/local/motif/lib -lXm -lXt -lX11 -lsocket -lnsl -lgen
d16 1
d18 1
d20 1
a20 1
LIB             = -ldl -lelf
d22 2
a23 1
PROGS		= tst.so
d25 2
a26 1
TARGETS		= $(PROGS)
d28 2
d31 2
a32 3
default:
	@@make clean
	@@make tst.so
d35 1
a35 41
	/bin/rm -f core $(TARGETS)

cleanall:
	@@make clean
	/bin/rm -f *.o *.so

tst.o: tst.c
	@@echo
	@@echo "Making 'tst.o'"
	$(CC) $(CFLAGS) -c tst.c -o tst.o

tst.so: tst.o
	@@echo
	@@echo "Making 'tst.so'"
	$(LD) $(LD-FLAGS) tst.o -o tst.so


memo: memo.c
	@@echo
	@@echo "Making 'memo'"
	$(CC) $(CFLAGS) memo.c $(XLIBS) -o memo

xtst.o: xtst.c
	@@echo
	@@echo "Making 'xtst.o'"
	$(CC) $(CFLAGS) -c xtst.c -o xtst.o

xtst.so: xtst.o
	@@echo
	@@echo "Making 'xtst.so'"
	$(LD) -B dynamic xtst.o $(XLIBS) -o xtst.so

nickb.o: nickb.c
	@@echo
	@@echo "Making 'nickb.o'"
	$(CC) $(CFLAGS) -c nickb.c -o nickb.o

nickb.so: nickb.o
	@@echo
	@@echo "Making 'nickb.so'"
	$(LD) $(LD-FLAGS) nickb.o -o nickb.so
@


