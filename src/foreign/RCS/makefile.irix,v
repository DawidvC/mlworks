1.1
log
@new unit
Irix specific makefile for building FI test programs.
@
text
@d4 3
d8 2
d11 1
a11 1
XLIBS           = -L/usr/local/X11R5/lib -L/usr/local/motif/lib -lXm -lXt -lX11 -lsocket -lnsl -lgen
d14 2
a15 3
# LD-FLAGS        = -Bdynamic -shared -exports
LD-FLAGS        = -Bdynamic -ignore_unresolved -call_shared -elf -z

d20 1
a20 1
PROGS		= tst.so ebug
d22 1
a22 1
TARGETS		= $(PROGS)
d24 2
d27 2
a28 3
default:
	@@make clean
	@@make tst.so
d31 1
a31 49
	/bin/rm -f core $(TARGETS) *.o *.so

cleanall:
	@@make clean
	/bin/rm -f *.o *.so

tst.o: tst.c
	@@echo
	@@echo "Making 'tst.o'"
	$(CC) $(CFLAGS) -c tst.c -o tst.o

tst.so: tst.o
	@@echo
	@@echo "Making 'tst.so'"
	$(LD) $(LD-FLAGS) -o tst.so tst.o

ebug.o: ebug.c
	@@echo "Making 'ebug.o'"
	$(CC) $(CFLAGS) -o ebug.o -c ebug.c

ebug.so: ebug.o
	@@echo "Making 'ebug.so'"
	$(LD) $(LD-FLAGS) -o ebug.so ebug.o


memo: memo.c
	@@echo
	@@echo "Making 'memo'"
	$(CC) $(CFLAGS) memo.c $(XLIBS) -o memo

xtst.o: xtst.c
	@@echo
	@@echo "Making 'xtst.o'"
	$(CC) $(CFLAGS) -c xtst.c -o xtst.o

xtst.so: xtst.o
	@@echo
	@@echo "Making 'xtst.so'"
	$(LD) -B dynamic xtst.o $(XLIBS) -o xtst.so

nickb.o: nickb.c
	@@echo
	@@echo "Making 'nickb.o'"
	$(CC) $(CFLAGS) -c nickb.c -o nickb.o

nickb.so: nickb.o
	@@echo
	@@echo "Making 'nickb.so'"
	$(LD) $(LD-FLAGS) nickb.o -o nickb.so
@


