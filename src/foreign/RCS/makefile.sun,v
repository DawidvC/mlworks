text
@d4 3
d8 2
d18 10
a27 1
PROGS		= tst.so memo
d29 2
a30 1
TARGETS		= $(PROGS)
d32 2
d35 2
a36 3
default:
	@@make clean
	@@make tst.so
d39 1
a39 1
	/bin/rm -f core $(TARGETS) *.o *.so
a40 48
cleanall:
	@@make clean
	/bin/rm -f *.o *.so

tst.o: tst.c
	@@echo
	@@echo "Making 'tst.o'"
	$(CC) $(CFLAGS) -c tst.c -o tst.o

tst.so: tst.o
	@@echo
	@@echo "Making 'tst.so'"
	$(LD) $(LD-FLAGS) tst.o -o tst.so

memo: memo.c
	@@echo
	@@echo "Making 'memo'"
	$(CC) $(CFLAGS) memo.c $(XLIBS) -o memo

xtst.o: xtst.c
	@@echo
	@@echo "Making 'xtst.o'"
	$(CC) $(CFLAGS) -c xtst.c -o xtst.o

xtst.so: xtst.o
	@@echo
	@@echo "Making 'xtst.so'"
	$(LD) $(LD-FLAGS) xtst.o -o xtst.so

xtst1.o: xtst1.c
	@@echo
	@@echo "Making 'xtst1.o'"
	$(CC) $(CFLAGS) -c xtst1.c -o xtst1.o

xtst1.so: xtst1.o
	@@echo
	@@echo "Making 'xtst1.so'"
	$(LD) $(LD-FLAGS) xtst1.o -o xtst1.so

nickb.o: nickb.c
	@@echo
	@@echo "Making 'nickb.o'"
	$(CC) $(CFLAGS) -c nickb.c -o nickb.o

nickb.so: nickb.o
	@@echo
	@@echo "Making 'nickb.so'"
	$(LD) $(LD-FLAGS) nickb.o -o nickb.so
@


