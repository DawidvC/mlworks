@(*  ==== FOREIGN INTERFACE : C INTERFACE ====
 *
 *  Copyright (C) 1995 Harlequin Ltd.
 *
 *  Implementation
 *  --------------
 *  This provides a FI interface in terms of a data/storage model similar
 *  to that used in C.
 *
 *  Revision Log
 *  ------------
 *  $Log$
 *
 *)

require "c_function";
require "c_interface";

functor CInterface(

   structure CFunction    : C_FUNCTION

) : C_INTERFACE =
  struct

    open CFunction
    open CStruct
    open CSignature
    open CData
    open CHostel

    open FITypes

  end;
@
