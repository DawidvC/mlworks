1.4
log
@add xtst.so
@
text
@# Example makefile (SunOS variant)

CC 		= gcc
# for cc case, use:
#CC		= cc

CFLAGS		= -ansi -pedantic -g -Wall
# for cc case, use:
#CFLAGS		= -ansiposix -g

LD              = ld
LDFLAGS		= -Bdynamic

PROGS		= hello.so random.so regexp.so xtst.so

all : $(PROGS)

%.o : %.c
	$(CC) $(CFLAGS) -c $< -o $@@

%.so : %.o
	$(LD) $(LDFLAGS) $< -o $@@

clean:
	$(RM) *.so *.o@


