(**
    General tests for the Match compiler.
    Last updated : Tue Jan 22 15:35:46 1991
**)

fun ptest name x =
    let
      val _ = output (std_out, "\n" ^ name ^ ": " ^ x ^ "\n") ;
    in
      test_mc x
    end

val it = ptest "Test1"
"val it = fn (x,y,1,1) => x+y | (1,foo,2,2) => foo+8 | (2,1,1,3) => 8" ;

val it = ptest "Test2"
"val it = fn [] => 0 | [[]] => 1 | [[x]] => x+1 | [(x::_)] => x+2 | ([]::_) => 2 | ((x::_)::_) => ~x | _ => 5" ;

val it = ptest "Test3"
"val it = fn 1 => 0 | 2 => 1 | 1 => 2 | 3 => 21 | 1 => 8" ;

val it = ptest "Test4"
"val it = fn (1, _) => 0 | (_, 1) => 1 | (1, 2) => 2" ;

val it = ptest "Test5"
"val it = fn (1, _) => 0 | (_, 1) => 1 | (1, 2) => 2 | (_, _) => 3" ;

val it = ptest "Test6"
"datatype s = NIL|CONS of (bool*bool*bool) val it = fn CONS(true, _, true) => 1 | CONS(false, _, true) => 1 | NIL => 2 | CONS(true, _, false) => 1 | CONS( false, _, false) => 3 | CONS(_, false, _) => 4" ;

val it = ptest "Test7"
"val it = fn ((true, true), _) => 0 | ((true, false), true) => 1 | ((false, true), true) => 2 | ((false, false), true) => 3 | (_, false) => 4 | (_, true) => 5" ;

(*
datatype g = x | y | z val it = fn (w, true) => 0 | (x, true) => 1 | (y, true) => 2 | (_, false) => 3 | (w, true) => 9 | (x, true) => 10;
*)
@


