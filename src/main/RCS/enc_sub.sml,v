  structure FuntypeHashTable : NEWHASHTABLE
  structure ConstypeHashTable : NEWHASHTABLE
  structure RectypeHashTable : NEWHASHTABLE
  structure DecFuntypeHashTable : NEWHASHTABLE
  structure DecConstypeHashTable : NEWHASHTABLE
  structure DecRectypeHashTable : NEWHASHTABLE
  structure TynameHashTable : NEWHASHTABLE
  structure ValenvHashTable : NEWHASHTABLE
a27 16
  sharing type FuntypeHashTable.Key = DataTypes.Type
  sharing type ConstypeHashTable.Key = DataTypes.Type
  sharing type RectypeHashTable.Key = DataTypes.Type
  sharing type DecFuntypeHashTable.Key = int
  sharing type DecConstypeHashTable.Key = int
  sharing type DecRectypeHashTable.Key = int
  sharing type TynameHashTable.Key = DataTypes.Tyname
  sharing type ValenvHashTable.Key = DataTypes.Valenv
  sharing type FuntypeHashTable.Value = int
  sharing type ConstypeHashTable.Value = int
  sharing type RectypeHashTable.Value = int
  sharing type DecFuntypeHashTable.Value = DataTypes.Type
  sharing type DecConstypeHashTable.Value = DataTypes.Type
  sharing type DecRectypeHashTable.Value = DataTypes.Type
