functor StringPropVar(structure Agt: SAGENT):SPROPVAR =
struct

  structure A = Agt

  datatype propvar = mk_propvar of string

  fun eq (mk_propvar n) (mk_propvar m) = n=m

  fun mkstr (mk_propvar n) = n

