functor StringName() : SNAME =
struct
    datatype name = Name of string

    fun mkname (s) = Name(s)
    fun mkstr(Name(s)) = s

    fun eq(Name(s), Name(t)) = s = t
    fun le(Name(s), Name(t)) = s <= t
end
@


