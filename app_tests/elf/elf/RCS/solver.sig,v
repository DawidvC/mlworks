(* The solver for Elf *)

signature SOLVER =
sig
  structure Term : TERM
  structure Constraints : CONSTRAINTS  sharing Constraints.Term = Term

  val solve : Term.term
                 -> bool
	         -> Constraints.constraint
		 -> (Constraints.constraint -> unit)
		 -> unit

  structure Switch : SWITCH

  val trace : int -> unit
  val untrace : unit -> unit

end  (* signature SOLVER *)
@


