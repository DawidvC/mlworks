text
@(*
 *
 * $Log$
 *
 *)
RCS "$Id: HMLogic.str,v 1.3 1994/12/03 14:50:05 pxs Exp $";
functor HMLogic ( structure A : ACT;
                  structure V : VAR ) : HMLOGIC =
struct
   structure A = A
   structure V = V

   datatype modality = modlist of A.act list
                     | negmodlist of A.act list

   datatype prop = True
                 | Var of V.var
                 | Not of prop
                 | And of prop * prop
                 | Nec of modality * prop
                 | Max of V.var * prop
end


@


1.1
log
@new unit
Putting application tests into hope and daily
@
text
@@
