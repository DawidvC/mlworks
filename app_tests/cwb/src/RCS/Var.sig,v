RCS "$Id: Var.sig,v 1.4 1994/12/03 14:57:20 pxs Exp $";
(************************************ Var ************************************)
(*                                                                           *)
(*  This is the definitions of "Variables".           Joachim Parrow Sept-86 *)
(*                                                                           *)
(*****************************************************************************)

signature VAR =
sig
   eqtype var

   val mkvar : string -> var
   val mkstr : var -> string

   val eq    : var * var -> bool
   val le    : var * var -> bool

   val hashval : var -> int
end

@


