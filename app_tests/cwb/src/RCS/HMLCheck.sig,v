(********************************** HMLSat ***********************************)
(*                                                                           *)
(* This file contains the signature for model checking on predefined graphs. *)
(*                                                                           *)
(*****************************************************************************)

signature HMLCHECK =
sig
   structure PG : POLYGRAPH

   eqtype mcinfo
   type prop

   val setmcinfo : PG.agent -> mcinfo
   val check : (mcinfo PG.state ref * mcinfo PG.state ref list) -> prop -> bool
end


@


