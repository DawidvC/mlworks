require "basic_types";

signature MARK =
    sig
	exception MARK of string

	val show  : BasicTypes.Mark -> string
	val showL : (BasicTypes.Mark * BasicTypes.Mark) list -> string

	val read  : string -> BasicTypes.Mark
	val readL : string -> (BasicTypes.Mark * BasicTypes.Mark) list

    end
@
