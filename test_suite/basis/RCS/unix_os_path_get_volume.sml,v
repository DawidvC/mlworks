open OS.Path;

val a = getVolume "/" = "";

val b = getVolume "//" = "";

val c = getVolume "a//b/c/" = "";

val d = getVolume "./" = "";

val e = getVolume "../" = "";

val f = getVolume "" = "";

val g = getVolume "C:" = "";
@


