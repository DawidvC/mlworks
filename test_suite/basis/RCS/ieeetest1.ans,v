@new unit
Answer file for rounding mode test
@
text
@a0 44
val it : unit = ()
structure IEEEReal =
  struct
    eqtype decimal_approx = {digits: int list, exp: int, kind: float_class, sign: bool}
    datatype float_class =
      INF |
      NAN of nan_mode |
      NORMAL |
      SUBNORMAL |
      ZERO
    datatype nan_mode =
      QUIET |
      SIGNALLING
    datatype real_order =
      EQUAL |
      GREATER |
      LESS |
      UNORDERED
    datatype rounding_mode =
      TO_NEAREST |
      TO_NEGINF |
      TO_POSINF |
      TO_ZERO
    val EQUAL : IEEEReal.real_order
    val GREATER : IEEEReal.real_order
    val INF : IEEEReal.float_class
    val LESS : IEEEReal.real_order
    val NAN : IEEEReal.nan_mode -> IEEEReal.float_class
    val NORMAL : IEEEReal.float_class
    val QUIET : IEEEReal.nan_mode
    val SIGNALLING : IEEEReal.nan_mode
    val SUBNORMAL : IEEEReal.float_class
    val TO_NEAREST : IEEEReal.rounding_mode
    val TO_NEGINF : IEEEReal.rounding_mode
    val TO_POSINF : IEEEReal.rounding_mode
    val TO_ZERO : IEEEReal.rounding_mode
    val UNORDERED : IEEEReal.real_order
    exception Unordered
    val ZERO : IEEEReal.float_class
    val fromString : string -> {digits: int list, exp: int, kind: IEEEReal.float_class, sign: bool} option = fn
    val getRoundingMode : unit -> IEEEReal.rounding_mode = fn
    val setRoundingMode : IEEEReal.rounding_mode -> unit = fn
    val toString : {digits: int list, exp: int, kind: IEEEReal.float_class, sign: bool} -> string = fn
  end
@


