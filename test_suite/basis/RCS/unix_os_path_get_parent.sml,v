@(* Copyright (c) 1996 Harlequin Ltd.

Result: OK

 *
 * Test OS.Path.getParent.  All tests should return true.
 * 
 * Revision Log
 * ------------
 *
 * $Log$
 *)

Shell.Build.loadSource "unix.__os";

open OS.Path;

val a = getParent "/" = "/";

val b = getParent "a" = ".";

val c = getParent "a/" = "a/..";

val d = getParent "a///" = "a///..";

val e = getParent "a/b" = "a";

val f = getParent "a/b/" = "a/b/..";

val g = getParent ".." = "../..";

val h = getParent "." = "..";

val i = getParent "../" = "../.."

val j = getParent "./" = "./..";

val k = getParent "" = "..";

val l = getParent "/abc" = "/";

val m = getParent "/abc." = "/";

val p = getParent "/abc.." = "/";

val q = getParent "/abc/.." = "/abc/../..";

val r = getParent "../.." = "../../..";

val r = getParent "/abc/." = "/abc/..";
@
