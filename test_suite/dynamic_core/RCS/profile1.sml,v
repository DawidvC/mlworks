fun fromList [] = []
  | fromList _ = raise Div

val fromList =
  MLWorks.Profile.profile
    (MLWorks.Profile.OPTIONS
      {scan = 3, depth = 2, select = ["add'"]})
    std_out
    fromList;

fun mkList 0 = []
|   mkList n = n :: mkList (n-1);

val it = (fromList(mkList 42)) handle _ => fromList([]);
