val x = 0w0
val y = 0w9
val z = 0w16
val a = 0wx10

val 0w0 = x
val 0wx0 = x

fun f 0w0 = 1
  | f 0w2 = 2
  | f 0wx10 = 4
  | f _ = 3

val e = f 0w16

val g = f 0wx11

val h = (x + y) * z - a

val i = z div y

val j = z mod y
@


