Shell.Options.set(Shell.Options.Compiler.optimizeTailCalls, false);

fun do_test i = 
  let
    val results = MLWorks.Internal.Array.array (10, 0);

    fun print_results i =
      let
        val score = real (MLWorks.Internal.Array.sub (results, i)) ;
      in
        ()
      end
  in
    print_results 0
  end
@


