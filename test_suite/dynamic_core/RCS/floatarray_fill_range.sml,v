          of [3,2] => MLWorks.IO.output(MLWorks.IO.std_out, "Pass2\n")
           | _ => MLWorks.IO.output(MLWorks.IO.std_out, "Fail2\n")
d44 2
a45 2
          of [3,11] => MLWorks.IO.output(MLWorks.IO.std_out, "Pass3\n")
           | _ => MLWorks.IO.output(MLWorks.IO.std_out, "Fail3\n")
d53 2
a54 2
          of [253,2] => MLWorks.IO.output(MLWorks.IO.std_out, "Pass4\n")
           | _ => MLWorks.IO.output(MLWorks.IO.std_out, "Fail4\n")
