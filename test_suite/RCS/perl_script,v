1.3
log
@Failing to match MLWorks prompts with no trailing spaces.
@
text
@d3 2
a4 1
# Magic perl script generated by running s2p on our old 'sed' script.
d20 2
d25 15
a39 10
while ($ARGV[0] =~ /^-/) {
    $_ = shift;
  last if /^--/;
    if (/^-n/) {
	$nflag++;
	next;
    }
    die "I don't recognize this switch: $_\\n";
}
$printit++ unless $nflag;
d41 1
a41 1
$\ = "\n";		# automatically add newline on print
d43 1
a43 10
LINE:
while (<>) {
    chop;
    if (1..1) {
        $printit = 0;
        next LINE;
    }
    if (/^Setting/) {
        $printit = 0;
        next LINE;
a44 6
    if (/^Use/) {
        $printit = 0;
        next LINE;
    }
    s/MLWorks-1>> *//g;
    s/MLWorks-1> *//g;
d46 1
a46 6
continue {
    if ($printit)
	{ print; }
    else
	{ $printit++ unless $nflag; }
}
@


